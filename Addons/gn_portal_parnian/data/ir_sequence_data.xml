<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences for sale.order -->
        <record id="seq_parnian_translation_branch" model="ir.sequence">
            <field name="name">Parnian Translation Branch</field>
            <field name="code">parnian.translation.branch</field>
            <field name="prefix">B_</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_parnian_translation_entry" model="ir.sequence">
            <field name="name">Parnian Translation Entry</field>
            <field name="code">parnian.translation.entry</field>
            <field name="prefix">E_</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_parnian_translation_issue" model="ir.sequence">
            <field name="name">Parnian Translation Issue</field>
            <field name="code">parnian.translation.issue</field>
            <field name="prefix">I_</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
